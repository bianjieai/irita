{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "iris-service-result",
	"description": "IRIS Service Result Schema",
	"type": "object",
	"properties": {
	  "code": {
		"description": "result code",
		"type": "integer",
		"enum": [200, 400, 500]
	  },
	  "message": {
		"description": "result message",
		"type": "string"
	  }
	},
	"additionalProperties": false,
	"required": [
	  "code",
	  "message"
	]
}
